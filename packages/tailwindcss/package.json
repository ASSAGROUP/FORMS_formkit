{
  "name": "@formkit/tailwindcss",
  "repository": "https://github.com/formkit/formkit",
  "version": "1.0.0-beta.15",
  "description": "A FormKit plugin for Tailwind that supplies custom variants based on input state",
  "keywords": [
    "forms",
    "inputs",
    "styling",
    "css",
    "tailwind",
    "tailwindcss"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "contributors": [
    "Andrew Boyd <andrew@formkit.com>",
    "Gustavo Fenilli <gustavofenilli@gmail.com>"
  ],
  "license": "MIT",
  "type": "module",
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  },
  "dependencies": {
    "@formkit/themes": "1.0.0-beta.15"
  },
  "devDependencies": {
    "@types/tailwindcss": "^3.0.10"
  }
}